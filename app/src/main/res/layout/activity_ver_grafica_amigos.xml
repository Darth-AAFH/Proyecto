<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/drawerlayout"
    tools:context=".ui.VerGraficaAmigos">

    <LinearLayout
        android:id="@+id/chart2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="290dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp">
    </LinearLayout>
    <TextView

        android:layout_marginTop="0dp"
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Grafica semanal"
        android:paddingTop="60dp"
        android:layout_marginLeft="150dp"
        tools:layout_editor_absoluteY="42dp" />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

        <TextView
            android:id="@+id/textViewDia1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="25dp"
            android:layout_marginTop="305dp"
            android:text="L"
            android:textColor="#FFFFFF"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewDia2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="305dp"
            android:layout_marginEnd="10dp"
            android:text="M"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toStartOf="@+id/textViewDia4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewDia3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="305dp"
            android:text="M"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toStartOf="@+id/textViewDia4"
            app:layout_constraintStart_toEndOf="@+id/textViewDia2"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewDia4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="305dp"
            android:text="J"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewDia5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="305dp"
            android:text="V"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toStartOf="@+id/textViewDia6"
            app:layout_constraintStart_toEndOf="@+id/textViewDia4"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewDia6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="305dp"
            android:layout_marginStart="10dp"
            android:text="S"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textViewDia4"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewDia7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="305dp"
            android:layout_marginEnd="25dp"
            android:text="D"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />



    </androidx.constraintlayout.widget.ConstraintLayout>
    <include
        layout="@layout/app_bar_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        app:menu="@menu/activity_main_drawer"
        />
</androidx.drawerlayout.widget.DrawerLayout>
